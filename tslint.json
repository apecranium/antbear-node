{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "arrow-parens": false,
    "eofline": true,
    "indent": [true, "spaces", 2],
    "no-console": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single"],
    "trailing-comma": false,
    "interface-name": false
  }
}
